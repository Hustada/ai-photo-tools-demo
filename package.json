    "@vercel/kv": "^3.0.0",
    "axios": "^1.9.0",
    "openai": "^5.0.1",
    "pip": "^0.0.1",
    "pnpm": "^10.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
