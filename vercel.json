{
  "framework": null,
  "devCommand": "vite --port $PORT",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cron/aggregate-feedback",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/cron/evolve-prompts",
      "schedule": "0 0 * * 0"
    }
  ]
}